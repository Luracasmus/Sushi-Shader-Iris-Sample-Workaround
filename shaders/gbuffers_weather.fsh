#version 120

varying vec4 color;
varying vec4 texcoord;

uniform sampler2D texture;

//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////

void main() {

/* DRAWBUFFERS:5 */

	vec4 tex = texture2D(texture, texcoord.xy)*color;
	gl_FragData[0] = tex;
}
